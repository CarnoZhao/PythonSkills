    e.preventDefault();
    $(this).closest('.alertMessage').fadeOut('slow')
});
})

if ( '/library/content/myworkspace_info_zh_CN.html'.indexOf('http://') == 0 || '/library/content/myworkspace_info_zh_CN.html'.indexOf('https://') == 0 ) {

    function timeout47d73299_6ead_42c5_b696_ce402ab99080() { 
        var myframe = $("#id47d73299_6ead_42c5_b696_ce402ab99080");
        var loaded = false;
        window.console && console.log("/library/content/myworkspace_info_zh_CN.html timeout check...");
        try {
            var iframeLoc = myframe.get(0).contentWindow.location;
            iframeLoc = String(iframeLoc);
            var windowLoc = String(window.location);
            window.console && console.log("iframe.location="+windowLoc);
            var ipos = -1;
            var slashcount = 0;
            for (var i=0; i<windowLoc.length; i++ ) {
                if ( windowLoc[i] == '/' ) slashcount++;
                if ( slashcount == 3 ) {
                    ipos = i;
                    break;
                }
            }
            if ( ipos == -1 ) {
                loaded = false;
            } else { 
                var prefix = windowLoc.substring(0, ipos);
                window.console && console.log("iframe.prefix="+prefix);
                loaded = iframeLoc.indexOf(prefix) == 0;
            }
        } catch(ex) {  // This is an expected/normal/hoped for occurance
            loaded = true;
        }
        if ( ! loaded ) {
            $('#alert47d73299_6ead_42c5_b696_ce402ab99080').show();
        } else {
            window.console && console.log('/library/content/myworkspace_info_zh_CN.html load success');
        }

    }

    var timer47d73299_6ead_42c5_b696_ce402ab99080 = false;
    $(document).ready(function() {
        timer47d73299_6ead_42c5_b696_ce402ab99080 = window.setTimeout("timeout47d73299_6ead_42c5_b696_ce402ab99080()",8000);
    });
} else {
    window.console && console.log('/library/content/myworkspace_info_zh_CN.html does not start with http:// or https://');
}
